body
	margin: 25px
	background-color: backgroundColor
	font-family: tahoma

h2, h4
	color: headerColor
	letter-spacing: 1.5px
	line-height: 1.5
	border-bottom: solid 1px headerColor

h5
	color: headerColor
	letter-spacing: 1.5px
	line-height: 1
	text-transform: uppercase
	border-bottom: solid 1px headerColor

p
	color: textColor
	letter-spacing: 0.25px
	line-height: 1.5

@media (max-width: 815px)
	h5
		line-height: 1.5

	p
		text-align: left
